<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.0.0-alfa-04</Version>
    <Authors>Marcin Gołębiowski</Authors>
    <Company>CFGToolkit</Company>
    <Description>An expression evaluator for .NET.
Support for C language expressions</Description>
    <Copyright>MIT</Copyright>
    <PackageProjectUrl>https://github.com/CFGToolkit/CFGToolkit.ExpressionEvaluator</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CFGToolkit/CFGToolkit.ExpressionEvaluator</RepositoryUrl>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CFGToolkit.AST" Version="1.0.0-beta-6" />
    <PackageReference Include="CFGToolkit.ExpressionParser" Version="1.0.0-alfa-04" />
    <PackageReference Include="CFGToolkit.ParserCombinator" Version="1.0.0-beta-15" />
  </ItemGroup>

</Project>
